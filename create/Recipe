
cp container/faq.zip .

unzip faq.zip ; rm faq.zip

mkdir install


cp container/todonotes/todonotes.zip .

unzip todonotes.zip ; rm -f todonotes.zip ; cd todonotes ; latex todonotes.ins ; cp todonotes.sty .. ; cd ..


cp container/tocloft/tocloft.zip .

unzip tocloft.zip ; rm -f tocloft.zip ; cd tocloft ; latex tocloft.ins ; cp tocloft.sty .. ; cd ..


cp container/microtype/microtype.zip .

unzip microtype.zip ; rm -f microtype.zip ; cd microtype ; latex microtype.ins ; cp microtype.sty .. ; cd ..


